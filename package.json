{
  "name": "prismy-session",
  "version": "3.0.0",
  "description": ":ledger: Session for prismy",
  "keywords": [
    "prismy",
    "session"
  ],
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "homepage": "https://github.com/prismyland/prismy-session",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./memory": {
      "types": "./dist/memory.d.ts",
      "require": "./dist/memory.js"
    },
    "./redis": {
      "types": "./dist/redis.d.ts",
      "require": "./dist/redis.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismyland/prismy-session.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc -P tsconfig.build.json",
    "lint": "prettier --check src/**/*.ts specs/**/*.ts",
    "format": "prettier --write src/**/*.ts specs/**/*.ts",
    "test": "npm run lint && npm run test-type && npm run test-coverage",
    "test-type": "tsc --noEmit",
    "test-api": "jest",
    "test-coverage": "jest --coverage",
    "codecov": "codecov",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "bugs": {
    "url": "https://github.com/prismyland/prismy-session/issues"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.8",
    "@types/uid-safe": "^2.1.5",
    "codecov": "^3.8.1",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "prettier": "^2.1.2",
    "prismy": "^4.0.0-6",
    "redis": "^4.6.12",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "prismy": ">= 4 < 5 || >= 4.0.0-0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "cookie-signature": "^1.2.1",
    "uid-safe": "^2.1.5"
  }
}
