{
  "name": "prismy-session",
  "version": "0.0.1-4",
  "description": "Session for prismy",
  "keywords": [
    "prismy",
    "session"
  ],
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "homepage": "https://github.com/prismyland/prismy-session",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "!dist/specs/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismyland/prismy-session.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "prettier --check src/**/*.ts examples/*/src/**/*.ts",
    "format": "prettier --write src/**/*.ts examples/*/src/**/*.ts",
    "test": "ava --verbose",
    "test-ci": "nyc ava && nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "bugs": {
    "url": "https://github.com/prismyland/prismy-session/issues"
  },
  "devDependencies": {
    "@types/got": "^9.4.4",
    "@types/node": "^12.0.2",
    "@types/test-listen": "^1.1.0",
    "ava": "^2.2.0",
    "codecov": "^3.5.0",
    "got": "^9.6.0",
    "micro": "^9.3.5-canary.1",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "prismy": "^0.11.0",
    "prismy-test-server": "^0.3.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "prismy": "^0.10.0"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}
